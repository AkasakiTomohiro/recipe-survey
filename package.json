{
  "name": "recipe-survey",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --cache --cache-location .eslintcache --cache-strategy content",
    "lint:markup": "yarn markuplint -p",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook",
    "storybook:test:update": "test-storybook --updateSnapshot",
    "storybook:test:coverage": "test-storybook --coverage && nyc report --reporter=text --reporter=lcov -t coverage/storybook --report-dir coverage/storybook",
    "build-storybook": "storybook build",
    "postinstallDev": "husky",
    "playwright:install": "yarn playwright install --with-deps"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@mui/styles": "^5.15.13",
    "i18next": "^23.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^14.1.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.19",
    "@markuplint/jsx-parser": "^4.3.0",
    "@markuplint/react-spec": "^4.1.0",
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/builder-vite": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/test-runner": "^0.17.0",
    "@types/node": "20.10.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "chromatic": "^11.0.8",
    "comment-json": "^4.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markuplint": "^4.3.0",
    "nyc": "^15.1.0",
    "playwright": "^1.42.1",
    "storybook": "^8.0.0",
    "storybook-react-i18next": "^2.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.3",
    "yarn": "4.0.0"
  }
}
